<div>

    {{#each randomBeer}}
<h3>Random Beers</h3>
  <img src="{{this.image_url}}" alt="Birra random" height="150px">
  <h2>{{this.name}}</h2>
  <p>{{this.tagline}}</p>
  <p>{{this.description}}</p>
  <p>Food pairing: {{this.food_pairing}}</p>
  <p>Brewer tips: {{this.brewers_tips}}</p>

    {{/each}}
</div>